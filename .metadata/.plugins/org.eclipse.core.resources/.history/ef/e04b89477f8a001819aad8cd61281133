
public class TestArea {

	public static void main(String[] args) {
		int i = 'A';
		System.out.println(i);
		char ch  = (char)65.25;
		System.out.println(ch); //When a floating-point
		//value is cast into a char, the floating-point
		//value is first cast into an int, which is then cast
		// into a char 
		
	}

}
