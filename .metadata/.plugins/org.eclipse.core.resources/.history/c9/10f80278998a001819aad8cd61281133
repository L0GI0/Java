import java.util.Scanner;
public class TestArea {

	public static void main(String[] args) {
		//Converting a numeric string into a numeber
		String intString = "123";
		String doubleString = "1.0 2.0 3.0";
		int intValue = Integer.parseInt(intString);
		//Converting a number into a string 
		String s = intValue + ""; 
		System.out.println(s);
		String concatenated1 = intString.concat(doubleString);
		String concatenated2 = intString + " " + doubleString;
		System.out.println("Concatenated1: " + concatenated1);
		System.out.println("Concatenated2: " + concatenated2);
	}

}
